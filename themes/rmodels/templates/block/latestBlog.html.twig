 <div class="news">
         <div class="container">
            <div class="row">
               <div class="col-md-12">
                  <div class="titlepage">
                     <h2>Letest News</h2>
                  </div>
               </div>
            </div>
            <div class="row">
               <div class="col-md-12 margin_top40">
               {% for article in article_data %}
                  <div class="row d_flex">
                     <div class="col-md-5">
                        <div class="news_img">
                           <figure><img src="{{article.article_img |file_url}}"></figure>
                        </div>
                     </div>
                     <div class="col-md-7">
                        <div class="news_text">
                           <h3>{{article.article_title}}</h3>
                           <span>{{article.article_date}}</span> 
                           <div class="date_like">
                              <!-- <span>Like </span><span class="pad_le">Comment</span> -->
                           </div>
                           <p>{{article.article_body}}</p>
                        </div>
                     </div>
                  </div>
               {% endfor %}
               </div>
            </div>
         </div>
      </div>