        <small class="text-black">Created by: {{model.agency_name}}</small>
        <img src="{{ active_theme_path()}}/images/verify-icon.png" style="
    position: relative;
    width: 9px;
    display: inline-flex;
">
      </div>
    </a>
  </div>
{% endfor %}
      
</div>  
